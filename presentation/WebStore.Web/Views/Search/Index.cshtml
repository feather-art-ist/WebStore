@model Book[]

@{
    ViewData["Title"] = "Search result";
}

<h1>Search result</h1>
@if(Model == null)
{
    <p>Something happend</p>
}
else
{
    @if(Model.Length == 0)
{
    <p>Nothing found</p>
}
else
{
    <ul>
        @foreach (var book in Model)
       {
           <li><a href = "@Url.Action("Index", "Book", new {id = book.Id})">@book.Title</a> </li>
       }
    </ul>
}
}
